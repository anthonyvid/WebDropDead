# Ignore bundler config
/.bundle
/vendor/

# Ignore the log files
/log/*
!/log/.keep

# Ignore tmp files
/tmp
/tmp/*

# Ignore the public system files
/public/system/*
!/public/system/.keep

# Ignore database files
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore uploaded files in production
/public/uploads

# Ignore node modules and yarn files
/node_modules
/yarn-error.log

# Ignore test and coverage reports
/test/tmp
/test/coverage

# Ignore environment variable files
.env
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore credentials file
/config/credentials.yml.enc

# Ignore backup files
*~

# Ignore files generated by Rails
/public/assets
/public/packs

# Ignore IDE files
.idea
*.swp
*.swo

# Ignore OS generated files
.DS_Store
Thumbs.db